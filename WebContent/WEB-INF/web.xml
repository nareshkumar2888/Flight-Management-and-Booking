<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login_check</servlet-name>
        <servlet-class>login_check</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin_data</servlet-name>
        <servlet-class>admin_data</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getFlightData</servlet-name>
        <servlet-class>getFlightData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Passenger</servlet-name>
        <servlet-class>Passenger</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login_check</servlet-name>
        <url-pattern>/login_check</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin_data</servlet-name>
        <url-pattern>/admin_data</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getFlightData</servlet-name>
        <url-pattern>/getFlightData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Passenger</servlet-name>
        <url-pattern>/Passenger</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
